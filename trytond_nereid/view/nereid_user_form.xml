<data>
    <xpath expr="/form" position="replace_attributes">
        <form string="Web User" />
    </xpath>
    <xpath expr="/form/group[@id='checkboxes']" position="after">
        <label name="email" />
        <field name="email" />
        <label name="password" />
        <field name="password" widget="char" />
        <label name="company" />
        <field name="company" />
        <label name="timezone" />
        <field name="timezone" />
    </xpath> 
</data>
